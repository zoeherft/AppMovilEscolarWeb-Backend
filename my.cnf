#my.cnf - Configuración de MySQL para desarrollo local
# IMPORTANTE: Este archivo es solo una plantilla.
# Copia este archivo como my.cnf.local y configura tus credenciales reales.
[client]
host=127.0.0.1
port = 3306
database = tu_base_de_datos
user = tu_usuario
password = tu_contraseña
default-character-set = utf8mb4